    client_max_body_size 10m;

    gzip on;
    gzip_disable “MSIE [1-6]\.(?!.*SV1)”;
    gzip_proxied any;
    gzip_buffers 16 8k;
    gzip_types text/plain text/css text/xml application/javascript application/x-javascript application/xml application/xml+rss text/javascript;
    gzip_vary on;

    set $upstream "";
